{
  "name": "jee_score",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "build": "rm -rf build && vite build && partytown copylib build/~partytown",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "gofmt -s -w api && prettier --plugin-search-dir . --write .",
    "local-test": "serve build"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.16.3",
    "@types/js-yaml": "^4.0.5",
    "@types/markdown-it": "^12.2.3",
    "autoprefixer": "^10.4.14",
    "date-picker-svelte": "^2.4.3",
    "eslint": "^8.40.0",
    "js-yaml": "^4.1.0",
    "markdown-it": "^13.0.1",
    "postcss": "^8.4.23",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "svelte": "^3.59.1",
    "svelte-check": "^2.10.3",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.5"
  },
  "type": "module",
  "dependencies": {
    "@builder.io/partytown": "^0.7.6",
    "@fontsource/roboto": "^4.5.8",
    "svelte-icons-pack": "^2.1.0"
  }
}
