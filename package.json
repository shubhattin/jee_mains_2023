{
  "name": "jee_score",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build && partytown copylib build/client/~partytown",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write .",
    "local-test": "serve build"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.2.0",
    "@sveltejs/kit": "1.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/markdown-it": "^12.2.3",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.33.0",
    "js-yaml": "^4.1.0",
    "markdown-it": "^13.0.1",
    "postcss": "^8.4.21",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "sass": "^1.58.0",
    "svelte": "^3.55.1",
    "svelte-check": "^2.10.3",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.2.6",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1"
  },
  "type": "module",
  "dependencies": {
    "@builder.io/partytown": "^0.7.5",
    "svelte-icons-pack": "^2.0.0"
  }
